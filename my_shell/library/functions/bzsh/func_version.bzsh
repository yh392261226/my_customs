function git_submodule_add() {                                                          # Desc: function:git_submodule_add: Gitç‰ˆæœ¬æ–‡ä»¶ä¸­å¢åŠ å­æ¨¡å—
    check_is_git || return
    if [ $# -ne 2 ]; then
        echo "Usage: git_submodule_add <submodule_name> <git_url>"
        return
    fi
    git submodule add $2 $1
    git commit -m "Add $1 submodule"
    git push
}
