            fi
            echo "alias_${word}_end" >> $TMP_A2Z_ALIASES_FILE
        fi
    done

    if [ "all" = "$1" ] && [ "" != "$1" ]; then
        bat $TMP_A2Z_ALIASES_FILE
    else
        # åˆ©ç”¨fzf
        seq 1 26 |  awk '{printf "%c\n", NR+96}' | fzf $FZF_CUSTOM_PARAMS \
        --header="$(_buildFzfHeader '' 'custom_aliases_from_a2z')" \
