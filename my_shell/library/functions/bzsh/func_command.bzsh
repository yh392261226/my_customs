    # åˆ›å»ºä¸´æ—¶æ–‡ä»¶
    local TMP_A2Z_ALIASES_FILE=$(mktemp)
    # å°†æ‰€æœ‰a-zçš„åˆ«åå†…å®¹å†™å…¥ä¸´æ—¶æ–‡ä»¶
    if [ ! -f $TMP_A2Z_ALIASES_FILE ]; then
        echo "Tmpfile does not exists !"
        return 1
    fi

    hr "â¬³" > $TMP_A2Z_ALIASES_FILE
    echo -n "[ $(echo -e {a..z}) ]\n" >> $TMP_A2Z_ALIASES_FILE
