export fzf_transformer_filter_contents="rg --color=always --line-number --no-heading '' "

export fzf_transformer='
    lines=$(( FZF_LINES - FZF_MATCH_COUNT - 1 ))
    if [[ $FZF_MATCH_COUNT -eq 0 ]]; then
        echo "change-preview-window:hidden"
    elif [[ $lines -gt 10 ]]; then
        echo "change-preview-window:$lines"
    elif [[ $lines -le 3 ]]; then
        echo "change-preview-window:40"
    elif [[ $FZF_PREVIEW_LINES -gt 0 ]]; then
        echo "change-preview-window:40"
    fi
'

export fzf_transformer_input_switcher='
    if [[ $FZF_INPUT_STATE = enabled ]]; then
        echo "rebind(j,k,/)+hide-input"
    elif [[ $FZF_KEY = enter ]]; then
        echo accept
    else
        echo abort
    fi
'

export fzf_transformer_footer_action='
    if [[ $FZF_CLICK_FOOTER_WORD =~ Help ]]; then
        echo "change-preview-label( ‚ï¢ Help Infomation ‚ïü )"
        echo "preview:(${MYRUNTIME}/customs/bin/_previewer \"help\")"
    elif [[ $FZF_CLICK_FOOTER_WORD =~ Copy ]]; then
        echo "execute-silent(echo -n \{} | pbcopy)+abort"
    elif [[ $FZF_CLICK_FOOTER_WORD =~ Open ]]; then
        echo "execute:open \{}"
    fi
'

export fzf_transformer_history_searcher='
    search_query=$(FZF_DEFAULT_OPTS="" && cat $HOME/.fzf_history | fzf --sync --history="$HOME/.fzf_history" +m --tac --no-sort --height=40% --layout=reverse --border-label="‚ï¢ HISTORY ‚ïü" --header="‚ñ≤ ÈÄâÊã©ÂéÜÂè≤Êü•ËØ¢ ‚ñº" --prompt="ÂéÜÂè≤ËÆ∞ÂΩï ‚ùØ ")
    [[ -n "$search_query" ]] && echo "change-query:$search_query"
'

export fzf_transformer_search_swap_type='
    if [ -f $TMP_FZF_SEARCH_SWAP_FILE ]; then
        local action=$(cat $TMP_FZF_SEARCH_SWAP_FILE)
        case $action in
            "all")          
                echo 'files'        > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Search All ‚ïü)+change-ghost(Type Search Words ...)+change-pointer( ‚û§)+reload($fzf_transformer_filter_all)" 
                ;;
            "files")        
                echo 'directories'  > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Files Only ‚ïü)+change-ghost(Type Filter Files Words ...)+change-pointer( ‚áù)+reload($fzf_transformer_filter_files)" 
                ;;
            "directories")  
                echo 'hiddens'      > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Directories Only ‚ïü)+change-ghost(Type Filter Directories Words ...)+change-pointer( ‚áõ)+reload($fzf_transformer_filter_directories)" 
                ;;
            "hiddens")      
                echo 'images'       > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Hiddens Only ‚ïü)+change-ghost(Type Filter Hidden Files and Directories Words ...)+change-pointer( ‚á¢)+reload($fzf_transformer_filter_hiddens)" 
                ;;
            "images")       
                echo 'medias'       > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Images Only ‚ïü)+change-ghost(Type Filter Images Words ...)+change-pointer( ‚á•)+reload($fzf_transformer_filter_images)" 
                ;;
            "medias")       
                echo 'documents'    > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Medias Only ‚ïü)+change-ghost(Type Filter Medias Words ...)+change-pointer( ‚áí)+reload($fzf_transformer_filter_medias)" 
                ;;
            "documents")    
                echo 'languages'    > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Documents Only ‚ïü)+change-ghost(Type Filter Documents Words ...)+change-pointer( ‚á™)+reload($fzf_transformer_filter_documents)" 
                ;;
            "languages")    
                echo 'contents'     > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Develop Languages Files ‚ïü)+change-ghost(Type Filter Develop Languages Words ...)+change-pointer( ‚ÜÆ)+reload($fzf_transformer_filter_languages)" 
                ;;
            "contents")     
                echo 'archives'     > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Contents ‚ïü)+change-ghost(Type Filter Contents Words ...)+change-pointer( ‚á®)+reload($fzf_transformer_filter_contents)" 
                ;;
            "archives")     
                echo 'all'          > $TMP_FZF_SEARCH_SWAP_FILE 
                echo "change-list-label(‚ï¢ Filter Archives Only ‚ïü)+change-ghost(Type Filter Archives Words ...)+change-pointer( ‚Üí)+reload($fzf_transformer_filter_archives)" 
                ;;
        esac
  else
        echo "all" > $TMP_FZF_SEARCH_SWAP_FILE && echo "change-list-label(‚ï¢ Search All ‚ïü)+change-ghost(Type Search Words ...)+change-pointer( ‚û§)+reload($fzf_transformer_filter_all)"
  fi
'

