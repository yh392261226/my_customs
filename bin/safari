#!/usr/bin/env bash
########################################
## Desc: Safari浏览器打开网址
## Author: 杨浩
########################################
BROWSERPATH="/Applications/Safari.app"
DEFAULTURL="https://www.google.com/"

[[ ! -d "$BROWSERPATH/" ]] && echo "Does not found Safari " && exit 1

[ "" = "$1" ] && url=$DEFAULTURL || url=$1
/usr/bin/open -a "$BROWSERPATH" "$url"
