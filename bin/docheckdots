#!/bin/bash
## Desc: 验证$MYRUNTIME下的dotfiles中是否已有自定义的设置连接文件set_to_my_symlink
## Author: 杨浩
MYRUNTIME=$(cat $HOME/.myruntime)
for i in $(ls -A $MYRUNTIME/ | grep '_dotfiles'); do
    if [ ! -f $MYRUNTIME/$i/set_to_my_symlink ];then
        echo $i
    else
        echo $i "has not set link file"
    fi
done
