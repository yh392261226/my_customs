#!/usr/bin/env bash
###Desc: atom which
###Author: Json
COMMANDBIN=/usr/local/bin/atom
if [ -f $COMMANDBIN ]; then
    if [ "" != "$1" ]; then
        command -v "$@" > /dev/null 2>&1
        [[ "$?" = "1" ]] && echo "Command $@ does not exists !" && return 1
        if [ "$(type $1 | grep 'a shell function from')" = "" ]; then
            ls -l  `which "$1"`
        else
            ls -l  $(type "$1" | awk '{print $NF}')
        fi
    else
    	$COMMANDBIN `pwd`
    fi
fi
